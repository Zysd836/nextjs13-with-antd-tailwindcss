{
	"name": "nextjs13-with-antd-tailwindcss",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write \"**/*.{js,ts,tsx,mdx}\" --cache",
		"check": "prettier --check \"**/*.{js,ts,tsx,mdx}\" --cache"
	},
	"dependencies": {
		"@ant-design/cssinjs": "^1.6.1",
		"antd": "^5.2.3",
		"clsx": "^1.2.1",
		"dayjs": "^1.11.7",
		"lucide-react": "^0.115.0",
		"next": "13.2.3",
		"next-intl": "2.11.0-beta.16",
		"next-themes": "^0.2.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tailwind-merge": "^1.10.0"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/node": "18.13.0",
		"@types/react": "18.0.27",
		"autoprefixer": "^10.4.13",
		"eslint": "7.32.0",
		"eslint-config-next": "13.1.6",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-tailwindcss": "^3.10.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"tailwindcss": "^3.2.7",
		"typescript": "4.9.5"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"npm run lint -- --quiet --fix"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
